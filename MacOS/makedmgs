cd
# rm -rf deployment
rm deployment-6.1/netgen/CMakeCache.txt
rm deployment-6.1/ngsolve/CMakeCache.txt
rm -rf install-6.1/Netgen.app
mkdir deployment-6.1
cd deployment-6.1
mkdir netgen
cd netgen
rm netgen*.dmg
rm ngsuite*.dmg


export MACOSX_DEPLOYMENT_TARGET=10.9
cmake /Users/joachim/gitlab/netgen-6.1 -DBoost_INCLUDE_DIR=/usr/local/boost_1_60_0 -DBoost_LIBRARY_DIR=/usr/local/boost_1_60_0/stage/lib -DPYTHON_EXECUTABLE=/Library/Frameworks/Python.framework/Versions/3.5/bin/python3.5 -DPYTHON_INCLUDE_DIR=/Library/Frameworks/Python.framework/Versions/3.5/include/python3.5m -DPYTHON_LIBRARY=/Library/Frameworks/Python.framework/Versions/3.5/lib/libpython3.5.dylib  -DCMAKE_CXX_COMPILER=$myCXX -DCMAKE_C_COMPILER=$myCC  -DCMAKE_LINKER=$myCXX -DCMAKE_CXX_FLAGS="-Wno-deprecated-declarations" -DCMAKE_C_FLAGS=""  -DUSE_NATIVE_ARCH=OFF -DUSE_GUI=ON -DUSE_PYTHON=ON -DINSTALL_DEPENDENCIES=ON -DINSTALL_DIR=/Users/joachim/install-6.1 -DTIX_LIBRARY=$HOME/Library/Tcl/Tix8.4.3 -DTK_DND_LIBRARY=$HOME/Library/Tcl/tkdnd2.8 -DTK_INCLUDE_PATH=/Library/Frameworks/Tk.framework/Headers -DTK_LIBRARY=/library/Frameworks/Tk.framework -DTCL_INCLUDE_PATH=/Library/Frameworks/Tcl.framework/Headers -DTCL_LIBRARY=/library/Frameworks/Tcl.framework
make -j 5
make install
NETGEN_BUNDLE_NAME=netgen make bundle


cd ..
mkdir ngsolve
cd ngsolve

cmake /Users/joachim/gitlab/ngsolve-6.1  -DINSTALL_DIR=/Users/joachim/install-6.1/Netgen.app -DCMAKE_CXX_COMPILER=$myCXX -DCMAKE_C_COMPILER=$myCC -DCMAKE_LINKER=$myCXX -DNETGEN_SOURCE_DIR=/Users/joachim/gitlab/netgen-6.1  -DCMAKE_CXX_FLAGS=" -msse4 -Wno-deprecated-declarations -I/Library/Frameworks/Python.framework/Versions/3.5/lib/python3.5/site-packages/numpy/core/include" -DCMAKE_C_FLAGS=""  -DUSE_NATIVE_ARCH=OFF  -DUSE_MKL=OFF -DUSE_PYTHON=ON -DBoost_INCLUDE_DIR=/usr/local/boost_1_60_0 -DBoost_LIBRARY_DIR=/usr/local/boost_1_60_0/stage/lib -DPYTHON_EXECUTABLE=/Library/Frameworks/Python.framework/Versions/3.5/bin/python3.5 -DPYTHON_INCLUDE_DIR=/Library/Frameworks/Python.framework/Versions/3.5/include/python3.5m -DPYTHON_LIBRARY=/Library/Frameworks/Python.framework/Versions/3.5/lib/libpython3.5.dylib -DUSE_UMFPACK=ON  -DINSTALL_DEPENDENCIES=OFF -DTK_INCLUDE_PATH=/Library/Frameworks/Tk.framework/Headers -DTK_LIBRARY=/library/Frameworks/Tk.framework -DTCL_INCLUDE_PATH=/Library/Frameworks/Tcl.framework/Headers -DTCL_LIBRARY=/library/Frameworks/Tcl.framework -DINSTALL_DIR=/Users/joachim/install-6.1

make -j 5
make install
cd ../netgen
NETGEN_BUNDLE_NAME=ngsuite make bundle

scp *.dmg schoeberl@auroraold.asc.tuwien.ac.at:public_html/downloads

